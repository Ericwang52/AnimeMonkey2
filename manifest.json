{
    "name":"Anime Monkey 2",
    "version":"1.0",
    "browser_action":
    {
      "default_popup": "./index.html",
      "default_title": "Anime Monkey 2"
      
    },
    "icons": {
        "16":"./images/icon16.png",
        "48": "./images/icon48.png",
       "128": "./images/icon128.png" },
    "manifest_version": 2,
    "permissions":
    [
      "alarms", 
      "notifications",
      "background",
      "storage"
    ],
    "background": 
    {
      "scripts": ["background.js"],
      "persistent": true
    }
}