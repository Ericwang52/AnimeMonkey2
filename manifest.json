{
    "name":"Anime Monkey 2",
    "version":"1.0",
    "browser_action":
    {
      "default_popup": "./index.html",
      "default_title": "Anime Monkey 2"
      
    },
    "icons":
    {
      "16": "./images/hoopshub16.png",
      "48": "./images/hoopshub48.png",
      "128": "./images/hoopshub128.png"
    },
    "manifest_version": 2,
    "permissions":
    [
      "alarms", 
      "notifications",
      "background",
      "storage"
    ],
    "background": 
    {
      "scripts": ["background.js"],
      "persistent": true
    }
}